#include <iostream>
#include <fstream>

using namespace std;

const char CDfv[] = "Duomenys.txt";
const char CRfv[] = "Rezultatai.txt";


int main()
{
	int n, m;
	double kaina, suma = 0;
	double visaKaina = 0;

	ifstream fd(CDfv);
	ofstream fr(CRfv);

	fd >> n >> m; 

	for (int i = 0; i < n; i++)
	{
		suma = 0;

		for (int j = 0; j < m; j++)
		{
			fd >> kaina;

			suma = suma + kaina;
		}

		fr << i + 1 << " " << suma << endl;

		visaKaina = visaKaina + suma;
	}

	fr << visaKaina;

	return 0;
}


